			j$('#phone-contact-submit').prop('disabled', true);
            if(j$('#phone_contact_name').val()==''){
                Alert.error('El nombre es requerido');
                habilitarButtons();
            }
            else if(j$('#phone_contact_phone').val()==''){
                Alert.error('El teléfono es requerido');
                habilitarButtons();
            }
            else if(j$('#phone_contact_phone').val().length < 6){
                Alert.error('El teléfono debe contener al menos 7 caracteres');
                habilitarButtons();
            }
            else if(j$('#phone_contact_phone').val()!='' && isNaN(j$('#phone_contact_phone').val()) == true){
                Alert.error('El teléfono solo debe contener números');
                habilitarButtons();
            }
            else {
                var i = j$('[name="phone_contact_name"]').val();
                var r = j$('[name="phone_contact_phone"]').val();
                PhoneContact.init(); 
                var leadInsertAteInme = new SObjectModel.LeadGateway();
                var leadAteInme = {};
                leadAteInme.LastName = j$('#phone_contact_name').val();
                leadAteInme.Phone = j$('#phone_contact_phone').val();
                leadAteInme.LeadSource = 'Marketing';					
            	leadAteInme.SubOrigenCandidato__c = 'Atención Inmediata';
                
                leadInsertAteInme.create(leadAteInme,function(err,res){
                if(err){
                    console.log(err);
                    console.log('No inserto ' + err);
                }            
                else{
					Alert.message(PhoneContact.alertBody({name: i,phone: r}));
                    habilitarButtons();
                }
            }); 
                
                habilitarButtons();
            }
        }
        
        function habilitarButtons(){
            j$('#cerrarAlert').on('click', function(){
                j$('#phone-contact-submit').prop('disabled', false);
            });
            j$('#aceptarAlert').on('click', function(){
                j$('#phone-contact-submit').prop('disabled', false);
            });
        }
        </script>
    </body>
    
</apex:page>