                lead.EstadoOtro__c = j$('#estado_input').val(); 
            }
            else{
            	lead.Estado__c = ubicacion.estado;
                lead.Municipio__c = ubicacion.municipio;
            }
            
            //var contUbicacion = 0;
            
            lead.Phone = j$('#telCasa').val();
            lead.MobilePhone = j$('#telCelular').val(); //
            lead.Email = j$('#mail').val();
            
            var nivel = j$('#nivel-remote option:selected').val();
            var nivelSplit = nivel.split('-');
